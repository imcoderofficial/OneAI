{
  "name": "oneai",
  "productName": "OneAI",
  "version": "0.1.0",
  "description": "Minimal Electron app for OneAI",
  "author": "imcoderofficial <support@imcoderofficial.com>",
  "homepage": "https://imcoder.in/",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --win"
  },
  "devDependencies": {
    "electron": "^37.4.0",
    "electron-builder": "^24.6.0"
  },
  "build": {
    "appId": "com.imcoderofficial.oneai",
  "companyName": "imcoderofficial",
    "productName": "OneAI",
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": ["nsis"],
      "icon": "assets/logo.ico"
    }
  }
}
